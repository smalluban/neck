{
  "name": "neck",
  "description": "Create apps with Backbone faster!",
  "author": "smalluban",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/smalluban/neck.git"
  },
  "keywords": [
    "backbone",
    "backbonejs",
    "plugin",
    "neck",
    "neckjs",
    "mvc",
    "binding",
    "data-binding",
    "nested-views"
  ],
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build -p",

{
  "name": "neck",
  "description": "Create apps with Backbone faster!",
  "author": "smalluban",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/smalluban/neck.git"
  },
  "keywords": [
    "backbone",
    "backbonejs",
    "plugin",
    "neck",
    "neckjs",
    "mvc",
    "binding",
    "data-binding",
    "nested-views"
  ],
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build",
    "test": "brunch build && ./node_modules/.bin/mocha-phantomjs lib/test/index.html && ./node_modules/.bin/poncho --reporter lcov lib/test/index.html | ./node_modules/.bin/coveralls",
    "test-mocha": "brunch build && ./node_modules/.bin/mocha-phantomjs lib/test/index.html"
  },
  "dependencies": {
    "javascript-brunch": "<= 1.8",
    "coffee-script-brunch": "<= 1.8",
    "uglify-js-brunch": "<= 1.8",
    "auto-reload-brunch": "<= 1.8",
    "css-brunch": "<= 1.8.0",
    "clean-css-brunch": "<= 1.8.0",
    "mocha-phantomjs": "~3.1.6",
    "poncho": "~0.1.1",
    "coveralls": "~2.10.0"
  }
}

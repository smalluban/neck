    "test-mocha": "brunch build && ./node_modules/.bin/mocha-phantomjs dist/test/index.html"
  },
  "dependencies": {
    "auto-reload-brunch": "^2.7.1",
    "bower": "^1.8.8",
    "brunch": "^2.10.17",
    "clean-css-brunch": "^2.10.0",
    "coffee-script-brunch": "^1.7.3",
    "coveralls": "~2.11.1",
    "css-brunch": "^2.10.0",
    "mocha-phantomjs": "~3.1.6",
    "poncho": "~0.1.1",
    "uglify-js-brunch": "^2.10.0"
  }
}
